{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {"text": "Rice exports by major exporting countries"},
  "width": 650,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/Aurora0815/Project2/main/Main%20countries%20Rice.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "country_or_area",
      "type": "nominal",
      "title": "Major exporters of rice"
    },
    "y": {"field": "quantity(kg)", "type": "quantitative"},
    "color": {
      "field": "Rice Type",
      "type": "nominal",
      "scale": {
        "domain": [
          "Milled Rice",
          "Husked Rice",
          "Paddy/Rough Rice",
          "Broken Rice"
        ],
        "range": ["#3182bd", "#17becf", "#31a354", "#393b79"]
      },
      "title": "Rice type"
    }
  },
  "config": {}
}