{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Value of Broken Rice Exports from 6 Continents",
  "width": 650,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/Aurora0815/Project2/main/Value-of-Exports-in-Rice.csv"
  },
  "mark": "area",
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year"},
    "y": {"aggregate": "sum", "field": "Trade Value"},
    "color": {"field": "Continent", "scale": {"scheme": "category20"}},
    "tooltip": [
      {"field": "Continent", "type": "nominal"},
      {"aggregate": "sum", "field": "Trade Value", "type": "quantitative"}
    ]
  },
  "config": {}
}